void reverse(char* str)
{
	char* first = str;
	char* last;
	char temp;

	//find last character of string
	while(*last != 0)
		last++;

	//step back from null terminator
	last--;

	//while first is before last
	//swap first and last
	while(first < last)
	{
		temp = *first;
		*first = *last;
		*last = temp
	}

	return;
}
